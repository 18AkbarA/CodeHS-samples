public String reverse(String str)
{
       
       String finalString = "";
       int StrLength = str.length();
       
       for(int i = 0; i < StrLength - 1; i++){
           int charNumb = StrLength - (1+ i);
           finalString+= "" + str.charAt(charNumb);
       }
       
       return finalString;
       
}
